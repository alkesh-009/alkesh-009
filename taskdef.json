{
  "family": "node-hello-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::403395453326:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "node-hello-container",
      "image": "REPLACE_ME",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
